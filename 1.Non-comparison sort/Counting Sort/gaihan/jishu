#include <stdio.h>
#include <stdlib.h>
void jishu(int a[1000],int n);
int main() {
	int gh,i;
	int gg[1000];
	scanf("%d",&gh);
	for(i=0;i<gh;i++)
	{
		scanf("%d",&gg[i]);
	} 
	jishu(gg,gh);
	return 0;
}

void jishu(int a[1000], int n) 
{  
  int i;
    int b[1000] = { 0 };  
    int c[10] = { 0 };  
  
    for (i = 0; i < n; i++)  
    {  
        c[a[i]] = c[a[i]] + 1;  
    }  
  
    for (i = 1; i <10; i++)  
    {  
        c[i] = c[i] + c[i - 1];  
    }  
  
    for (i = n-1; i>=0; i--)  
    {  
        b[c[a[i]]] = a[i];
        c[a[i]]--;  
    }
    for(i=1;i<=n;i++)
    {
    	printf("%d",b[i]);
	}
}
